      <inertial pos="0 0 0" euler="0 0 0" mass="10.0" diaginertia="0.001 0.001 0.001" />
    </body>

    <body name="lab_scene_floor" pos="0 0 -0.7" euler="0 0 0">
      <geom name="floor" pos="0 0 0" size="0 0 .05" type="plane" material="grid" condim="3" contype="0" conaffinity="0" />
    </body>
  </worldbody>
